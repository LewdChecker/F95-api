  "scripts": {
    "prettify": "prettier --config .prettierrc '{src,test}/**/*.ts' --write",
    "lint": "eslint --ext .ts,json src --fix",
    "pretest": "npm run build",
    "test": "echo 'Tests are broken'",
    "test:cjs": "mocha --require ts-node/register test/{unit,integration}/index.ts",
    "coverage": "echo 'Coverage is broken'",
    "coverage:cjs": "c8 --reporter=lcov --reporter=text npm run test:cjs",
