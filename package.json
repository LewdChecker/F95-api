{
  "name": "@millenniumearl/f95api",
  "main": "dist/index.js",
  "version": "2.0.0-beta.2",
  "author": "Millennium Earl",
  "description": "Unofficial Node JS module for scraping F95Zone platform",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MillenniumEarl/F95API.git"
  },
  "license": "MIT",
  "keywords": [
    "f95",
    "f95zone",
    "api",
    "scraping",
    "login",
    "game",
    "games",
    "data",
    "userdata",
    "user data"
  ],
  "scripts": {
    "lint": "eslint . --ext .ts",
    "prettier-format": "prettier --config .prettierrc {src,test}/**/*.ts --write",
    "test": "mocha --require ts-node/register test/{unit,integration}/index.ts",
    "coverage": "nyc --reporter=lcov --reporter=text npm run test",
    "run-example": "tsc && node --inspect ./dist/example.js"
  },
  "engines": {
    "node": ">=14.0"
  },
  "dependencies": {
    "@log4js-node/log4js-api": "^1.0.2",
    "await-semaphore": "^0.1.3",
    "axios": "^0.21.3",
    "axios-cache-adapter": "^2.7.3",
    "axios-cookiejar-support": "^1.0.1",
    "cheerio": "^1.0.0-rc.10",
    "class-validator": "^0.13.1",
    "js-sha256": "^0.9.0",
    "luxon": "^2.0.2",
    "tough-cookie": "^4.0.0"
  },
  "devDependencies": {
    "@millenniumearl/recaptcha-harvester": "^1.0.0",
    "@types/chai": "^4.2.15",
    "@types/chai-as-promised": "^7.1.3",
    "@types/inquirer": "^8.1.1",
    "@types/luxon": "^1.25.2",
    "@types/mocha": "^8.2.1",
    "@types/node": "^16.7.13",
    "@types/tough-cookie": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "chai": "^4.3.3",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^8.2.0",
    "electron": "^13.2.2",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.1.3",
    "inquirer": "^8.0.0",
    "lint-staged": "^10.5.4",
    "mocha": "^8.3.1",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --config .prettierrc {src,test}/**/*.ts --write",
      "git add"
    ]
  },
  "bugs": {
    "url": "https://github.com/MillenniumEarl/F95API/issues"
  },
  "homepage": "https://github.com/MillenniumEarl/F95API#readme"
}
